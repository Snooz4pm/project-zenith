{
    "version": 2,
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
    "env": {
        "ALPHA_VANTAGE_API_KEY": "@alpha_vantage_api_key",
        "NEON_DATABASE_URL": "@neon_database_url"
    }
}